// Login as depthead by default
curl -X POST http://localhost:5000/api/auth/login \
-H "Content-Type: application/json" \
-d '{
  "email": "jane.doe@example.com",
  "password": "strongpassword123"
}'

// List all requisitions as depthead
curl -X GET http://localhost:5000/api/requisition/all/ \
-H "Authorization: Bearer <YOUR_DEPTHEAD_TOKEN>"

// Ask for requisition
curl -X POST http://localhost:5000/api/requisition \
-H "Content-Type: application/json" \
-H "Authorization: Bearer <YOUR_DEPTHEAD_TOKEN>" \
-d '{
  "item": "Mouse",
  "quantity": 10
}'

// Login as Procurement officer 

curl -X POST http://localhost:5000/api/auth/login \
-H "Content-Type: application/json" \
-d '{
  "email": "test.procurement@example.com",
  "password": "testpassword123"
}'

// List all requisitions as procurement officer
curl -X GET http://localhost:5000/api/requisition/all/ \
-H "Authorization: Bearer <YOUR_PROCUREMENT_OFFICER_TOKEN>"

// Approve a requisition
curl -X PUT http://localhost:5000/api/requisition/<REQUISITION_ID> \
-H "Content-Type: application/json" \
-H "Authorization: Bearer <YOUR_PROCUREMENT_OFFICER_TOKEN>" \
-d '{
  "status": "Approved"
}'

